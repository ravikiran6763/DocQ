if (VSeeServerConnection.instance().getLoginState() == VSeeServerConnection.LoginState.LOGGED_IN) {


      Log.d("IF PART","CAME");

    VSeeNotificationType intentType = VSeeNotificationCenter.instance().getBundleNotificationType(getIntent().getExtras());
    switch (intentType) {
        case ONE_TO_ONE_CHAT:
        case GROUP_CHAT:
        case IN_CALL_CHAT:
        case WAITING_ROOM_CHAT:
            String chatId = VSeeNotificationCenter.instance().getBundleChatChatId(getIntent().getExtras());
            if (chatId == null) {

                break;
            }
            else {
                if (intentType == VSeeNotificationType.ONE_TO_ONE_CHAT) {
                    startActivity(VSeeChatManager.instance().getOneToOneChatIntent(chatId));
                } else if (intentType == VSeeNotificationType.GROUP_CHAT) {
                    startActivity(VSeeChatManager.instance().getGroupChatIntent(chatId));
                } else if (intentType == VSeeNotificationType.IN_CALL_CHAT) {
                    startActivity(VSeeChatManager.instance().getInCallChatIntent());
                } else if (intentType == VSeeNotificationType.WAITING_ROOM_CHAT) {
                    startActivity(VSeeChatManager.instance().getWaitingRoomChatIntent());
                }
                break;
            }
        case CALL: {
            startActivity(VSeeVideoManager.instance().getVideoLaunchIntent());
            break;
        }
        case MISSED_CALL: {
            break;
        }
        default: {

          Log.d("THE DEFAULT CLASS","CALLED");

            Intent intent = new Intent(this, LoginActivity.class);
            startActivity(intent);
            break;
        }
    }

}
else {


      Log.d("ELSE PART","CAME");


     NotificationCenter.instance().getLoginUUID();

    Intent intent = new Intent(this, com.greettelehealth.DoctorQuick.LoginActivity.class);
    startActivity(intent);
}

finish();

}
